#. type: Plain text
#: Pl_Editor.adoc:185
msgid "Texts can be simple strings or can include format symbols."
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:187
msgid "Format symbols are replaced by the actual values in Eeschema or Pcbnew."
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:189
msgid "They are like format symbols in printf function."
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:191
msgid "A format symbol is *%* followed by 1 letter."
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:193
msgid "The *%C* format has one digit (comment identifier)"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:195
msgid "Formats symbols are:"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:197
#, no-wrap
msgid "*%% = replaced by %*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:199
#, no-wrap
msgid "*%K = Kicad version*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:201
#, no-wrap
msgid "*%Z = paper format name (A4, USLetter ...)*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:203
#, no-wrap
msgid "*%Y = company name*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:205
#, no-wrap
msgid "*%D = date*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:207
#, no-wrap
msgid "*%R = revision*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:209
#, no-wrap
msgid "*%S = sheet number*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:211
#, no-wrap
msgid "*%N = number of sheets*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:213
#, no-wrap
msgid "*%Cx = comment (x = 0 to 9 to identify the comment)*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:215
#, no-wrap
msgid "*%F = filename*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:217
#, no-wrap
msgid "*%P = sheet path (sheet full name, for Eeschema)*\n"
msgstr ""

#. type: Plain text
#: Pl_Editor.adoc:219
#, no-wrap
msgid "*%T = title*\n"
msgstr ""

