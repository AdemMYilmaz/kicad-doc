"          <pins>\n"
"            <pin num=\"1\" type=\"passive\"/>\n"
"            <pin num=\"2\" type=\"passive\"/>\n"
"          </pins>\n"
"    Output pin list is ( <pin num> <net name> )\n"
"    something like\n"
"            ( 1 VCC )\n"
"            ( 2 GND )\n"
"-->\n"
"<xsl:template name=\"build_pin_list\" match=\"pin\">\n"
"    <xsl:param name=\"cmp_ref\" select=\"0\" />\n"
