#: Pl_Editor.adoc:225
msgid "Example:"
msgstr "Esempio:"

#. type: Plain text
#: Pl_Editor.adoc:227
msgid "\"Size: %Z\" displays \"Size: A4\" or \"Size: USLetter\""
msgstr ""
"\"Dimensione: %Z\" mostra \"Dimensione: A4\" o \"Dimensione: USLetter\""

